
<table class="horizontal_table" style="width: 100%; height: 10px;" border="1" cellspacing="0">




  

  <tbody>
  
      <tr>
        <td width="15%"><%= job.user.try(:player) %></td>
        <td width="35%"><%= job.text %></td>
        <td width="15%"><%= job.amount %></td>
        <td width="15%"><%= job.failed %></td>
        <td width="10%"><%= link_to image_tag("ic_settings_black_24dp.png"), edit_comp_job_path(job.comp, job) %></td>
        <td width="10%"><%= link_to image_tag("ic_delete_forever.png"), [job.comp, job], method: :delete, 
                                                       data: {confirm: 'Are you sure?'} %></td>
      </tr>
 
  </tbody>
</table>
