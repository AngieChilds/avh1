<p id="notice"><%= notice %></p>


<table>
  

  <tbody>
    <% @jobs.each do |job| %>
      <tr>
         <td><%= job.user.try(:player) %></td>
        <td><%= job.text %></td>
        <td><%= job.amount %></td>
     
      
        <td><%= link_to image_tag("ic_delete_forever.png"), [job.comp, job], method: :delete, 
                                                        data: {confirm: 'Are you sure?'} %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job Entry', new_comp_job_path %>
