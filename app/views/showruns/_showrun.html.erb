
<div id="showrun-list">

<table class="horizontal_table" style="width: 100%; height: 10px;" border="1" cellspacing="0">
<tbody>
  
    <tr valign="top" class="<%= cycle('list-line-odd', 'list-line-even') %>" >
    <td width="20%" ><%= showrun.user.try(:player) %></td>
    <td width="15%" ><%= showrun.other_player %></td>
    <td width="10%"><%= showrun.event %></td>
    <td width="10%"><%= showrun.spec_vote %></td>
    <td width="10%"><%= showrun.amt %></td>
    <td width="5%"><%= showrun.make_other %></td>
    <td width="20%"><%= showrun.level_to_make %></td>
    <td width="5%"><%= link_to image_tag("ic_settings_black_24dp.png"), edit_showblog_showrun_path(@showblog, showrun) %>
    <td width="5%" ><%= link_to image_tag("ic_delete_forever.png"), [showrun.showblog, showrun], method: :delete, 
                                                        data: {confirm: 'Are you sure?'} %></td>
     </tr>
     

</tbody>
</table>
</div>
